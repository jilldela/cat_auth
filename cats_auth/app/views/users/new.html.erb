<h1>Sign up, please!</h1>

<form action="<%= users_url  %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    Username: <input type="text" name="user[user_name]" value="<%= @user.user_name %>">
    <br>
    Password: <input type="password" name="user[password]" value="<%= @user.password  %>">
    <br>
    <input type="submit" value="Sign up!">
</form>
