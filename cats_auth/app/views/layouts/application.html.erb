<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <% if current_user %>
    <h1>Hi, <%= current_user.user_name %></h1>
  <% end %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

    <% if current_user %>
      <%= button_to("Logout", sessions_url, method: :delete) %>
    <% else %>
      <%= button_to("Log in", new_sessions_url, method: :get) %>
    <% end %>

<%= yield %>

</body>
</html>
