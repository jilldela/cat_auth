<h1>Log in to see cats, dawg!</h1>

<form action="<%= sessions_url %>" method="POST">
  <input type="hidden" name="authenticity_token"
    value="<%= form_authenticity_token %>">
  <input type="text" name="session[user_name]">
  <input type="password" name="session[password]">
  <input type="submit" value="Let's go, dawg">
</form>
